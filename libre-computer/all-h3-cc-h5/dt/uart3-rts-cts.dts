/dts-v1/;
/plugin/;

/ {
	compatible = "libretech,all-h3-cc-h3","libretech,all-h3-cc-h5";

	fragment@0 {
		target = <&uart3>;

		__overlay__ {
			pinctrl-names = "default";
			pinctrl-0 = <&uart3_pins>, <&uart3_rts_cts_pins>;
			uart-has-rtscts;
			status = "okay";
		};
	};
};

